{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "LensBorder-Pro",
  "version": "0.1.0",
  "identifier": "com.Redwood.lensborder",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
  {
    "title": "LensBorder Pro", 
    "width": 1000,
    "height": 800,
    "resizable": true,
    "fullscreen": false,
    "decorations": true,
    "alwaysOnTop": false
  }
],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
